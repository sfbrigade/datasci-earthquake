{
  "name": "nextjs-fastapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && fastapi dev api/index.py",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest",
    "dev-back": "npm run docker-front && npm run fastapi-dev",
    "dev-front": "npm run docker-back && npm run next-dev",
    "docker-back": "docker compose up -d db db_test backend --build",
    "docker-front": "docker compose up -d frontend --build",
    "clean": "npm run clean-front",
    "clean-front": "rm -rf .next && rm -rf node_modules && npm cache clean --force",
    "rebuild": "npm run rebuild-front",
    "rebuild-front": "npm run clean-front && npm install && npm run build"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/inter": "^5.1.0",
    "@mapbox/search-js-react": "^1.0.0-beta.22",
    "@types/geojson": "^7946.0.14",
    "@types/node": "^20.2.4",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "framer-motion": "^11.5.6",
    "mapbox-gl": "^3.7.0",
    "next": "^14.2.29",
    "postcss": "^8.4.23",
    "react": "18.2.7",
    "react-dom": "18.2.7",
    "react-icons": "^5.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/testing-library__user-event": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.29",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2"
  },
  "resolutions": {
    "react": "^18.2.7",
    "react-dom": "^18.2.7",
    "next": "14.2.29"
  }
}
